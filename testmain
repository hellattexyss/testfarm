local function TeleportAdd(num)

local args = { [1] = "Add", [2] = num }

TeleportEvent:FireServer(unpack(args))

task.wait(0.5)

TeleportEvent:FireServer("Chosen", nil, 1)

end

task.spawn(function()

while true do

TeleportAdd(3) TeleportAdd(2) TeleportAdd(1)

task.wait(0.3)

end

end)

