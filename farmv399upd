    getgenv().WebhookURL = _G.Webhook
end
getgenv().MaxServerTime = getgenv().MaxServerTime or 15

-- AutoExecute loader (your exact pattern)
pcall(function()
    if (rawget(_G, "AutoExecute") and _G.AutoExecute == true) and not _G.StopAuto then
        _G.StopAuto = false
        loadstring(game:HttpGet("https://raw.githubusercontent.com/hellattexyss/testfarm/refs/heads/main/farmv399upd"))()
    end
end)

